{
  "name": "@forten/tree-view",
  "version": "0.0.0",
  "description": "visual tree composer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fortenjs/forten.git"
  },
  "author": "Maia Taewana <maia@pawijs.org>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p setup/tsconfig.${NODE_ENV:=development}.json",
    "postbuild": "cp package.json ../../node_modules/@forten/tree-view",
    "clean": "rimraf .cache dist coverage ../../node_modules/@forten/tree-view",
    "prepare": "rimraf dist && cp -r ../../node_modules/@forten/tree-view/dist dist",
    "stories": "parcel serve --port 8888 --no-autoinstall src/stories/index.html",
    "test": "jest -c setup/jest.js",
    "test:watch": "jest -c setup/jest.js --watch --coverage false"
  },
  "dependencies": {
    "@forten/build": "next",
    "@forten/dragdrop": "next",
    "@forten/locale": "next",
    "@forten/theme": "next",
    "@forten/styled": "next",
    "@forten/tree": "next",
    "@forten/tree-type": "next",
    "polished": "^3.6.7"
  },
  "devDependencies": {
    "@forten/story": "next"
  }
}